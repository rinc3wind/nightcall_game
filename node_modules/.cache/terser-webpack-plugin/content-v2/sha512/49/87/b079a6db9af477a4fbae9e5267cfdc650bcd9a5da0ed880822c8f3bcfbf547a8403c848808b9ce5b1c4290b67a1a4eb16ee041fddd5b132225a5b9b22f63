ueTo\\n                this.progress = 0\\n                this.$emit('setDisabled', {\\n                    new: true,\\n                    save: true,\\n                    load: true\\n                })\\n            },\\n            keyUp(e) {\\n                if (e.key == 'ArrowRight') {\\n                    this.isKeyDown = false\\n                    clearInterval(this.interval)\\n                    this.interval = null\\n                }\\n            },\\n            keyDown(e) {\\n                if (this.interval == null && e.key == 'ArrowRight') {\\n                    this.isKeyDown = true\\n                    this.interval = setInterval(() => {\\n                        this.progress = this.progress + this.speed\\n\\n                        var random = Math.floor(Math.random() * (40))\\n                        if (random == 5) this.flash()\\n\\n                        if (this.progress >= 100) {\\n                            this.$emit('setStep', this.continueTo)\\n                            this.$emit('setDisabled', {\\n                                new: false,\\n                                save: false,\\n                                load: false\\n                            })\\n                        }\\n                    }, 100)\\n                }\\n            },\\n            flash() {\\n                this.flashColor = 'white'\\n                this.flashVisible = true\\n                setTimeout(() => {\\n                    this.flashVisible = false\\n                }, 140)\\n\\n                var flashDuration = Math.floor(Math.random() * (500)) + this.reactionTime\\n                setTimeout(() => {\\n                    this.flashColor = 'yellowgreen'\\n                    this.flashVisible = true\\n                    if (this.isKeyDown == true) {\\n                        if (this.player.cheats_enabled == false) this.progress = 0\\n                    }\\n                    setTimeout(() => {\\n                        this.flashVisible = false\\n                    }, 140)\\n                }, flashDuration)\\n            }\\n        }\\n    }\\n</script>\\n\\n<style>\\n    progress[value] {\\n        -webkit-appearance: none;\\n        appearance: none;\\n        width: 100%;\\n        height: 20px;\\n    }\\n    progress[value]::-webkit-progress-bar {\\n        background-color: black;\\n        border: 2px solid;\\n        border-color: yellowgreen;\\n    }\\n    progress[value]::-webkit-progress-value {\\n        background-color: yellowgreen;\\n    }\\n    .flash {\\n        background-color: yellowgreen;\\n        position: absolute;\\n        opacity: 0.7;\\n        top: 0;\\n        left: 0;\\n        width: 100vw;\\n        height: 100vh;\\n        /* top: 25%;\\n        left: 25%;\\n        width: 50vw;\\n        height: 50vh; */\\n    }\\n</style>\\n\\n\",\"import mod from \\\"-!../node_modules/cache-loader/dist/cjs.js??ref--12-0!../node_modules/thread-loader/dist/cjs.js!../node_modules/babel-loader/lib/index.js!../node_modules/cache-loader/dist/cjs.js??ref--0-0!../node_modules/vue-loader/lib/index.js??vue-loader-options!./Chapter3.vue?vue&type=script&lang=js&\\\"; export default mod; export * from \\\"-!../node_modules/cache-loader/dist/cjs.js??ref--12-0!../node_modules/thread-loader/dist/cjs.js!../node_modules/babel-loader/lib/index.js!../node_modules/cache-loader/dist/cjs.js??ref--0-0!../node_modules/vue-loader/lib/index.js??vue-loader-options!./Chapter3.vue?vue&type=script&lang=js&\\\"\",\"import { render, staticRenderFns } from \\\"./Chapter3.vue?vue&type=template&id=059f76ae&\\\"\\nimport script from \\\"./Chapter3.vue?vue&type=script&lang=js&\\\"\\nexport * from \\\"./Chapter3.vue?vue&type=script&lang=js&\\\"\\nimport style0 from \\\"./Chapter3.vue?vue&type=style&index=0&lang=css&\\\"\\n\\n\\n/* normalize component */\\nimport normalizer from \\\"!../node_modules/vue-loader/lib/runtime/componentNormalizer.js\\\"\\nvar component = normalizer(\\n  script,\