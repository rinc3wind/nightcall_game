;AAjCA,CAAA","sourcesContent":["<template>\n    <div id=\"app\">\n        <div>\n            <audio id=\"audio_player\" controls loop style=\"display: none;\">\n                <source src=\"./assets/untitled.mp3\" type=\"audio/mpeg\">\n            </audio>\n\n            <div class=\"settings\">\n                <b>Moznosti</b><br><br>\n                <div @click=\"togglePlayer\" class=\"setting\">\n                    <span v-if=\"audio_player_paused == true\">Zapnut hudbu</span>\n                    <span v-else>Vypnut hudbu</span>\n                </div>\n                <div @click=\"saveProgress()\" class=\"setting\">\n                    Ulozit hru\n                </div>\n                <div @click=\"loadProgress()\" class=\"setting\">\n                    Nacitat hru\n                </div>\n                <div @click=\"newGame()\" class=\"setting\">\n                    Nova hra\n                </div>\n            </div>\n\n            <div v-if=\"player.inventory.length > 0\" class=\"inventory\">\n                <b>Tvoj inventar</b><br><br>\n                <div v-for=\"item in player.inventory\" :key=\"item\">\n                    {{ item }}\n                </div>\n            </div>\n\n            <div class=\"note\" v-if=\"note != null\">\n                <div class=\"note_content\">\n                    <div style=\"margin-bottom: 50px;\">\n                        {{ note }}\n                    </div>\n                    <div @click=\"note = null\" class=\"choice\" style=\"text-align: center;\">\n                        {{ lang.continue }}\n                    </div>\n                </div>\n            </div>\n\n            <!--------- CHARACTER CREATION ---------------------------------------------->\n            <character-creation\n                v-if=\"chapter == 0\"\n                :player=\"player\"\n                :step=\"step\"\n                @setStep=\"step = $event\"\n                @setChapter=\"chapter = $event\">\n            </character-creation>\n\n            <!--------- CHAPTER 1 ------------------------------------------------------->\n            <chapter1\n                v-if=\"chapter == 1\"\n                :player=\"player\"\n                :step=\"step\"\n                @note=\"note = $event\"\n                @pickupItem=\"pickupItem($event)\"\n                @setStep=\"step = $event\"\n                @setChapter=\"chapter = $event\">\n            </chapter1>\n\n            <!--------- CHAPTER 2 ------------------------------------------------------->\n            <chapter2\n                v-if=\"chapter == 2\"\n                :player=\"player\"\n                :step=\"step\"\n                @note=\"note = $event\"\n                @pickupItem=\"pickupItem($event)\"\n                @setStep=\"step = $event\"\n                @setChapter=\"chapter = $event\">\n            </chapter2>\n        </div>\n        <!-- <div v-if=\"page_ready == false\" class=\"loading\">\n                NACITAVAM\n            </div> -->\n    </div>\n</template>\n\n<script>\n\n    import CharacterCreation from './CharacterCreation.vue'\n    import Chapter1 from './Chapter1.vue'\n    import Chapter2 from './Chapter2.vue'\n\n    export default {\n        name: 'app',\n        components: {\n            'character-creation': CharacterCreation,\n            'chapter1': Chapter1,\n            'chapter2': Chapter2\n        },\n        data() {\n            return {\n                page_ready: false,\n                player: {\n                    name: null,\n                    family_status: null,\n                    family_status_m: null,\n                    family_status_o: null,\n                    song: null,\n                    inventory: []\n                },\n                chapter: 0,\n                step: 0,\n                note: null,\n                audio_player: null,\n                audio_player_paused: true\n            }\n        },\n        mounted() {\n            var self = this\n            this.audio_player = document.getElementById('audio_player')\n            this.audio_player.oncanplaythrough = function () {\n                self.page_ready = true\n                document.querySelector('#app').style.display = 'block'\n            }\n        },\n        methods: {\n            pickupItem(data) {\n                if (data.item) this.player.inventory.push(data.item)\n                else if (data.items) {\n                    data.items.forEach(item => {\n                        this.player.inventory.push(item)\n                    })\n                }\n                if (data.note != null) this.note = data.note\n            },\n            togglePlayer() {\n                if (this.audio_player.paused) this.audio_player.play()\n                else this.audio_player.pause()\n                this.audio_player_paused = this.audio_player.paused\n            },\n            saveProgress() {\n                localStorage.setItem('player', JSON.stringify(this.player))\n                localStorage.setItem('chapter', this.chapter)\n                localStorage.setItem('step', this.step)\n                this.note = 'Hra ulozena'\n            },\n            loadProgress() {\n                if (localStorage.player && localStorage.step && localStorage.chapter) {\n                    this.player = JSON.parse(localStorage.player)\n                    this.step = parseFloat(localStorage.step)\n                    this.chapter = localStorage.chapter\n                    this.note = 'Hra nacitana'\n                } else {\n                    this.note = 'Nie je ulozena ziadna pozicia'\n                }\n            },\n            newGame() {\n                this.player = {\n                    name: null,\n                    family_status: null,\n                    family_status_m: null,\n                    family_status_o: null,\n                    song: null,\n                    inventory: []\n                },\n                this.chapter = 0\n                this.step = 0\n            }\n        }\n    }\n</script>\n\n<style>\n    @font-face {\n        font-family: pixel_font;\n        src: url(assets/pixel.ttf);\n    }\n\n    body {\n        background: black;\n    }\n\n    #app {\n        width: 660px;\n        margin-left: auto;\n        margin-right: auto;\n        margin-top: 60px;\n        text-align: justify;\n        font-family: pixel_font;\n        font-size: 18px;\n        color: greenyellow;\n    }\n\n    input {\n        width: 100%;\n        background: black;\n        color: greenyellow;\n        font-family: pixel_font;\n        font-size: 18px;\n    }\n\n    .choice {\n        font-weight: bold;\n        cursor: pointer;\n        color: white;\n        padding: 4px;\n    }\n    .choice:hover {\n        background-color: grey;\n        /* background-color: greenyellow; */\n        /* color: black; */\n    }\n\n    .inventory {\n        position: absolute;\n        left: 10px;\n    }\n\n    .settings {\n        position: absolute;\n        right: 10px;\n    }\n\n    .note {\n        position: fixed;\n        z-index: 1;\n        left: 0;\n        top: 0;\n        width: 100%;\n        height: 100%;\n        overflow: auto;\n    }\n\n    .note_content {\n        margin: 15% auto;\n        padding: 20px;\n        border: 1px solid greenyellow;\n        width: 30%;\n        background-color: rgba(0, 0, 0, 0.8);\n\n    }\n\n    .loading {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        font-family: pixel_font;\n        font-size: 18px;\n        color: greenyellow;\n        margin-top: 20%;\n    }\n\n    .setting {\n        cursor: pointer;\n    }\n\n    .setting:hover {\n        color: white;\n    }\n</style>"],"sourceRoot":"src"}]}